                },
                {
                    "field": "technical_review.date_initial_call_made",
                    "title": "Initial Call Made Date"
                }
            ]
        }
    },
    "facets": {
        "structural_variant.gnomadg_af": {
            "title": "gnomAD SV",
            "aggregation_type": "range",
            "number_step": "any",
            "order": 0,
            "grouping": "Allele Frequency",
            "hide_facet_counts": true,
            "ranges": [
                {
                    "from": 0,
                    "to": 0,
                    "label": "unobserved"
                },
                {
                    "from": 0,
                    "to": 0.001,
                    "label": "ultra-rare"
                },
                {
                    "from": 0,
                    "to": 0.01,
                    "label": "rare"
                },
                {
                    "from": 0.01,
                    "to": 1,
                    "label": "common"
                }
            ]
        },
        "structural_variant.unrelated_count": {
            "title": "CGAP Reference Frequency",
