        "structural_variant.gnomadg_af": {
            "title": "Allele Frequency",
            "order": 70,
            "sort_fields": [
                {
                    "field": "structural_variant.gnomadg_af",
                    "title": "gnomAD AF"
                },
                {
                    "field": "structural_variant.unrelated_count",
