                        "title": "Sample Genotype (Sample ID)",
                        "field_name": "samplegeno_sampleid",
                        "type": "string",
                        "do_import": true,
                        "vcf_field": "sample",
                        "source_name": "VCF",
                        "sub_embedding_group": "{\"key\": \"samplegeno\", \"title\": \"Sample Genotype\"}",
