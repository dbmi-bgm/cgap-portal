        "structural_variant.transcript.csq_gene.display_title": {
            "title": "Gene(s)",
            "order": 40
        },
        "associated_genotype_labels.proband_genotype_label": {
            "title": "Genotype",
            "order": 50,
            "sort_fields": [
                {
                    "field": "associated_genotype_labels.proband_genotype_label",
                    "title": "Proband GT"
