{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": [
        "institution",
        "project",
        "CALL_INFO",
        "variant",
        "file"
    ],
    "identifyingProperties": [
        "uuid",
        "aliases",
        "annotation_id"
    ],
    "additionalProperties": false,
    "mixinProperties": [
        {
            "$ref": "mixins.json#/schema_version"
        },
        {
            "$ref": "mixins.json#/uuid"
        },
        {
            "$ref": "mixins.json#/aliases"
        },
        {
            "$ref": "mixins.json#/submitted"
        },
        {
            "$ref": "mixins.json#/modified"
        },
        {
            "$ref": "mixins.json#/status"
        },
        {
            "$ref": "mixins.json#/attribution"
        },
        {
            "$ref": "mixins.json#/notes"
        },
        {
            "$ref": "mixins.json#/static_embeds"
        }
    ],
    "title": "Sample Variant",
    "description": "Schema for variant info for sample",
    "id": "/profiles/variant_sample.json",
    "properties": {
        "DP": {
            "title": "Coverage",
            "field_name": "DP",
            "type": "integer",
            "do_import": true,
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Variant Quality",
            "description": "Approximate read depth",
            "value_example": "47",
            "scope": "sample_variant",
            "schema_title": "Coverage",
            "column_order": 40,
            "facet_order": 8
        },
        "AF": {
            "title": "VAF",
            "field_name": "AF",
            "type": "number",
            "calculated_property": true,
            "do_import": true,
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Variant Quality",
            "description": "Variant allele fraction",
            "value_example": "0.2979",
            "scope": "sample_variant",
            "schema_title": "VAF",
            "column_order": 41,
            "facet_order": 9
        },
        "GQ": {
            "title": "Genotype Quality",
            "field_name": "GQ",
            "type": "integer",
            "do_import": true,
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Variant Quality",
            "description": "phred-scaled quality score that the genotype is accurate [min(99,PL(max)-PL(second))]",
            "value_example": "99",
            "scope": "sample_variant",
            "schema_title": "Genotype Quality",
            "column_order": 60,
            "facet_order": 10
        },
        "novoPP": {
            "title": "novoCaller",
            "field_name": "novoPP",
            "type": "number",
            "do_import": true,
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Genotype",
            "description": "Posterior probability of de novo mutation",
            "value_example": "1.0;0.9999999999999998",
            "scope": "sample_variant",
            "schema_title": "novoCaller",
            "facet_order": 11
        },
        "QUAL": {
            "title": "Variant Quality",
            "field_name": "QUAL",
            "type": "number",
            "do_import": true,
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Variant Quality",
            "description": "phred-scaled quality score for the assertion made in ALT (multi-sample)",
            "value_example": "302.6",
            "scope": "sample_variant",
            "schema_title": "Variant Quality"
        },
        "CALL_INFO": {
            "title": "Sample ID",
            "field_name": "CALL_INFO",
            "type": "string",
            "do_import": true,
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Genotype",
            "description": "Calling info",
            "value_example": "0/1:33,14:47:99:310,0,927",
            "scope": "sample_variant",
            "schema_title": "Sample ID"
        },
        "GT": {
            "title": "Genotype",
            "field_name": "GT",
            "type": "string",
            "do_import": true,
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Genotype",
            "description": "Genotype",
            "value_example": "0/1,0/2,1/2",
            "scope": "sample_variant",
            "schema_title": "Genotype"
        },
        "AD": {
            "title": "AD",
            "field_name": "AD",
            "type": "string",
            "do_import": true,
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Variant Quality",
            "description": "Allelic depth",
            "value_example": "33,14",
            "scope": "sample_variant",
            "schema_title": "AD"
        },
        "AD_ref": {
            "title": "AD (Ref)",
            "field_name": "AD_ref",
            "type": "integer",
            "calculated_property": true,
            "do_import": true,
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Variant Quality",
            "description": "Reference allelic depth",
            "value_example": "33",
            "scope": "sample_variant",
            "schema_title": "AD (Ref)"
        },
        "AD_alt": {
            "title": "AD (Alt)",
            "field_name": "AD_alt",
            "type": "integer",
            "calculated_property": true,
            "do_import": true,
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Variant Quality",
            "description": "Alternate allelic depth",
            "value_example": "14",
            "scope": "sample_variant",
            "schema_title": "AD (Alt)"
        },
        "PL": {
            "title": "Genotype Likelihoods (0/0,0/1,1/1)",
            "field_name": "PL",
            "type": "string",
            "do_import": true,
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Variant Quality",
            "description": "Phred-scaled likelihoods for genotype (Ref/Ref, Ref/Alt, Alt/Alt)",
            "value_example": "310,200,100",
            "scope": "sample_variant",
            "schema_title": "Genotype Likelihoods (0/0,0/1,1/1)"
        },
        "PGT": {
            "title": "Phasing information",
            "field_name": "PGT",
            "type": "string",
            "do_import": true,
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Genotype",
            "description": "Physical phasing haplotype information",
            "value_example": "0|1",
            "scope": "sample_variant",
            "schema_title": "Phasing information"
        },
        "PID": {
            "title": "Phasing ID",
            "field_name": "PID",
            "type": "string",
            "do_import": true,
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Genotype",
            "description": "Physical phasing ID information",
            "value_example": "985446_G_T",
            "scope": "sample_variant",
            "schema_title": "Phasing ID"
        },
        "PS": {
            "title": "Phaseing Set",
            "field_name": "PS",
            "type": "integer",
            "do_import": true,
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Genotype",
            "description": "Phasing set ID",
            "value_example": "985446",
            "scope": "sample_variant",
            "schema_title": "Phaseing Set"
        },
        "multiallele_samplevariantkey": {
            "title": "multiallele_samplevariantkey",
            "field_name": "multiallele_samplevariantkey",
            "type": "string",
            "do_import": true,
            "source_name": "MULTIALLELE",
            "source_version": "v0.4",
            "annotation_category": "Position",
            "description": "key for multi-allelic variant ",
            "scope": "sample_variant"
        },
        "samplegeno": {
            "title": "Sample Genotype",
            "type": "array",
            "items": {
                "title": "Sample Genotype",
                "type": "object",
                "properties": {
                    "samplegeno_numgt": {
                        "title": "samplegeno_numgt",
                        "field_name": "samplegeno_numgt",
                        "type": "string",
                        "do_import": true,
                        "source_name": "SAMPLEGENO",
                        "source_version": "v0.4",
                        "sub_embedding_group": "{\"key\": \"samplegeno\", \"title\": \"Sample Genotype\"}",
                        "annotation_category": "Genotype",
                        "description": "Sample genotype (number format)",
                        "value_example": "0/1",
                        "scope": "sample_variant"
                    },
                    "samplegeno_gt": {
                        "title": "samplegeno_gt",
                        "field_name": "samplegeno_gt",
                        "type": "string",
                        "do_import": true,
                        "source_name": "SAMPLEGENO",
                        "source_version": "v0.4",
                        "sub_embedding_group": "{\"key\": \"samplegeno\", \"title\": \"Sample Genotype\"}",
                        "annotation_category": "Genotype",
                        "description": "Sample genotype (nucletide format)",
                        "value_example": "G/T",
                        "scope": "sample_variant"
                    },
                    "samplegeno_ad": {
                        "title": "samplegeno_ad",
                        "field_name": "samplegeno_ad",
                        "type": "string",
                        "do_import": true,
                        "source_name": "SAMPLEGENO",
                        "source_version": "v0.4",
                        "sub_embedding_group": "{\"key\": \"samplegeno\", \"title\": \"Sample Genotype\"}",
                        "annotation_category": "Genotype",
                        "description": "Allelic depth",
                        "value_example": "21/10",
                        "scope": "sample_variant"
                    },
                    "samplegeno_sampleid": {
                        "title": "samplegeno_sampleid",
                        "field_name": "samplegeno_sampleid",
                        "type": "string",
                        "do_import": true,
                        "source_name": "SAMPLEGENO",
                        "source_version": "v0.4",
                        "sub_embedding_group": "{\"key\": \"samplegeno\", \"title\": \"Sample Genotype\"}",
                        "annotation_category": "Genotype",
                        "description": "Sample ID",
                        "scope": "sample_variant"
                    }
                }
            }
        },
        "cmphet": {
            "title": "Compound Het",
            "type": "array",
            "items": {
                "title": "Compound Het",
                "type": "object",
                "properties": {
                    "comhet_phase": {
                        "title": "comhet_phase",
                        "field_name": "comhet_phase",
                        "type": "string",
                        "do_import": true,
                        "source_name": "COMHET",
                        "source_version": "v1",
                        "sub_embedding_group": "{\"key\": \"cmphet\", \"title\": \"Compound Het\"}",
                        "scope": "sample_variant"
                    },
                    "comhet_gene": {
                        "title": "comhet_gene",
                        "field_name": "comhet_gene",
                        "type": "string",
                        "do_import": true,
                        "source_name": "COMHET",
                        "source_version": "v1",
                        "sub_embedding_group": "{\"key\": \"cmphet\", \"title\": \"Compound Het\"}",
                        "scope": "sample_variant"
                    },
                    "comhet_transcript": {
                        "title": "comhet_transcript",
                        "field_name": "comhet_transcript",
                        "type": "string",
                        "do_import": true,
                        "source_name": "COMHET",
                        "source_version": "v1",
                        "sub_embedding_group": "{\"key\": \"cmphet\", \"title\": \"Compound Het\"}",
                        "scope": "sample_variant"
                    },
                    "comhet_variant": {
                        "title": "comhet_variant",
                        "field_name": "comhet_variant",
                        "type": "string",
                        "do_import": true,
                        "source_name": "COMHET",
                        "source_version": "v1",
                        "sub_embedding_group": "{\"key\": \"cmphet\", \"title\": \"Compound Het\"}",
                        "scope": "sample_variant"
                    },
                    "comhet_impact": {
                        "title": "comhet_impact",
                        "field_name": "comhet_impact",
                        "type": "string",
                        "do_import": true,
                        "source_name": "COMHET",
                        "source_version": "v1",
                        "sub_embedding_group": "{\"key\": \"cmphet\", \"title\": \"Compound Het\"}",
                        "scope": "sample_variant"
                    }
                }
            }
        },
        "schema_version": {
            "default": "1"
        },
        "variant": {
            "title": "Variant",
            "type": "string",
            "linkTo": "Variant"
        },
        "file": {
            "title": "File",
            "description": "String Accession of the vcf file used in digestion",
            "type": "string"
        },
        "bam_snapshot": {
            "title": "Genome Snapshot",
            "description": "Link to Genome Snapshot Image",
            "type": "string",
            "format": "uri"
        },
        "annotation_id": {
            "title": "Annotation ID",
            "type": "string",
            "uniqueKey": true
        }
    },
    "columns": {
