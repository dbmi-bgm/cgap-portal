{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": [
        "institution",
        "project"
    ],
    "identifyingProperties": [
        "uuid",
        "aliases"
    ],
    "additionalProperties": false,
    "mixinProperties": [
        {
            "$ref": "mixins.json#/schema_version"
        },
        {
            "$ref": "mixins.json#/uuid"
        },
        {
            "$ref": "mixins.json#/aliases"
        },
        {
            "$ref": "mixins.json#/submitted"
        },
        {
            "$ref": "mixins.json#/modified"
        },
        {
            "$ref": "mixins.json#/status"
        },
        {
            "$ref": "mixins.json#/attribution"
        },
        {
            "$ref": "mixins.json#/notes"
        },
        {
            "$ref": "mixins.json#/static_embeds"
        }
    ],
    "title": "Sample Variant",
    "description": "Schema for variant info for sample",
    "id": "/profiles/variant_sample.json",
    "properties": {
        "QUAL": {
            "title": "QUAL",
            "vcf_name": "QUAL",
            "type": "number",
            "description": "Quality score",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "FILTER": {
            "title": "FILTER",
            "vcf_name": "FILTER",
            "type": "string",
            "description": "VQSR filter",
            "enum": [
                "PASS",
                "VQSRTrancheSNP99.00to99.90",
                "VQSRTrancheSNP99.90to100.00",
                "LowQual"
            ],
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "CALL_INFO": {
            "title": "CALL_INFO",
            "vcf_name": "CALL_INFO",
            "type": "string",
            "description": "Calling info",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "GT": {
            "title": "GT",
            "vcf_name": "GT",
            "type": "string",
            "description": "Genotype",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "AD": {
            "title": "AD",
            "vcf_name": "AD",
            "type": "string",
            "description": "Allelic depth",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "AD_ref": {
            "title": "AD_ref",
            "vcf_name": "AD_ref",
            "type": "integer",
            "description": "Reference allelic depth",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "AD_alt": {
            "title": "AD_alt",
            "vcf_name": "AD_alt",
            "type": "integer",
            "description": "Alternate allelic depth",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "DP": {
            "title": "DP",
            "vcf_name": "DP",
            "type": "integer",
            "description": "Approximate read depth",
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "column_priority": 6
        },
        "GQ": {
            "title": "GQ",
            "vcf_name": "GQ",
            "type": "integer",
            "description": "Genotype quality score",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "PL": {
            "title": "PL",
            "vcf_name": "PL",
            "type": "string",
            "description": "Phred-scaled likelihoods for genotype (Ref/Ref, Ref/Alt, Alt/Alt)",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "PGT": {
            "title": "PGT",
            "vcf_name": "PGT",
            "type": "string",
            "description": "Physical phasing haplotype information",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "PID": {
            "title": "PID",
            "vcf_name": "PID",
            "type": "string",
            "description": "Physical phasing ID information",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "PS": {
            "title": "PS",
            "vcf_name": "PS",
            "type": "integer",
            "description": "Phasing set ID",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "AF": {
            "title": "AF",
            "vcf_name": "AF",
            "type": "number",
            "description": "Sample allele fraction",
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "column_priority": 5
        },
        "RSTR": {
            "title": "RSTR",
            "vcf_name": "RSTR",
            "type": "string",
            "description": "Reference forward allele read count",
            "source_name": "novoCaller",
            "source_version": "2"
        },
        "novoCaller_Post_prob": {
            "title": "novoCaller_Post_prob",
            "vcf_name": "novoCaller_Post_prob",
            "type": "number",
            "description": "posterior probability",
            "source_name": "novoCaller",
            "source_version": "2"
        },
        "novoCaller_AF_unrel": {
            "title": "novoCaller_AF_unrel",
            "vcf_name": "novoCaller_AF_unrel",
            "type": "number",
            "description": "allele frequency in unrelated samples",
            "source_name": "novoCaller",
            "source_version": "2"
        },
        "schema_version": {
            "default": "1"
        },
        "sample": {
            "title": "Sample",
            "type": "string",
            "linkTo": "Sample"
        },
        "variant": {
            "title": "Variant",
            "type": "string",
            "linkTo": "Variant"
        }
    },
    "columns": {
        "DP": {
            "title": "DP",
            "aggregation_type": "stats"
        },
        "AF": {
            "title": "AF",
            "aggregation_type": "stats"
        },
        "variant.CHROM": {
            "title": "CHROM"
        },
        "variant.POS": {
            "title": "POS",
            "aggregation_type": "stats"
        },
        "variant.REF": {
            "title": "REF"
        },
        "variant.ALT": {
            "title": "ALT"
        },
        "variant.transcript.vep_consequence.display_title": {
            "title": "vep_consequence"
        },
        "variant.transcript.vep_symbol": {
            "title": "vep_symbol"
        },
        "variant.transcript.vep_hgvsc": {
            "title": "vep_hgvsc"
        },
        "variant.transcript.vep_variant_class": {
            "title": "vep_variant_class"
        },
        "variant.conservation_phastcons100": {
            "title": "conservation_phastcons100",
            "aggregation_type": "stats"
        },
        "variant.cadd_phred": {
            "title": "cadd_phred",
            "aggregation_type": "stats"
        },
        "variant.max_pop_af_af_popmax": {
            "title": "max_pop_af_af_popmax",
            "aggregation_type": "stats"
        }
    },
    "facets": {
        "variant.transcript.vep_consequence.display_title": {
            "title": "vep_consequence"
        },
        "variant.transcript.vep_symbol": {
            "title": "vep_symbol"
        },
        "variant.transcript.vep_variant_class": {
            "title": "vep_variant_class"
        },
        "variant.conservation_phastcons100": {
            "title": "conservation_phastcons100",
            "aggregation_type": "stats"
        },
        "variant.cadd_phred": {
            "title": "cadd_phred",
            "aggregation_type": "stats"
        },
        "variant.max_pop_af_af_popmax": {
            "title": "max_pop_af_af_popmax",
            "aggregation_type": "stats"
        }
    }
}