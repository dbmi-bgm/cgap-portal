{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": [
        "institution",
        "project"
    ],
    "identifyingProperties": [
        "uuid",
        "aliases"
    ],
    "additionalProperties": false,
    "mixinProperties": [
        {
            "$ref": "mixins.json#/schema_version"
        },
        {
            "$ref": "mixins.json#/uuid"
        },
        {
            "$ref": "mixins.json#/aliases"
        },
        {
            "$ref": "mixins.json#/submitted"
        },
        {
            "$ref": "mixins.json#/modified"
        },
        {
            "$ref": "mixins.json#/status"
        },
        {
            "$ref": "mixins.json#/attribution"
        },
        {
            "$ref": "mixins.json#/notes"
        },
        {
            "$ref": "mixins.json#/static_embeds"
        }
    ],
    "title": "Sample Variant",
    "description": "Schema for variant info for sample",
    "id": "/profiles/variant_sample.json",
    "properties": {
        "CALL_INFO": {
            "title": "CALL_INFO",
            "vcf_name": "CALL_INFO",
            "type": "string",
            "description": "Calling info",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "GT": {
            "title": "GT",
            "vcf_name": "GT",
            "type": "string",
            "description": "Genotype",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "AD": {
            "title": "AD",
            "vcf_name": "AD",
            "type": "string",
            "description": "Allelic depth",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "AD_ref": {
            "title": "AD_ref",
            "vcf_name": "AD_ref",
            "type": "integer",
            "description": "Reference allelic depth",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "AD_alt": {
            "title": "AD_alt",
            "vcf_name": "AD_alt",
            "type": "integer",
            "description": "Alternate allelic depth",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "DP": {
            "title": "DP",
            "vcf_name": "DP",
            "type": "integer",
            "description": "Approximate read depth",
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "column_priority": 6
        },
        "GQ": {
            "title": "GQ",
            "vcf_name": "GQ",
            "type": "integer",
            "description": "Genotype quality score",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "PL": {
            "title": "PL",
            "vcf_name": "PL",
            "type": "string",
            "description": "Phred-scaled likelihoods for genotype (Ref/Ref, Ref/Alt, Alt/Alt)",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "PGT": {
            "title": "PGT",
            "vcf_name": "PGT",
            "type": "string",
            "description": "Physical phasing haplotype information",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "PID": {
            "title": "PID",
            "vcf_name": "PID",
            "type": "string",
            "description": "Physical phasing ID information",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "PS": {
            "title": "PS",
            "vcf_name": "PS",
            "type": "integer",
            "description": "Phasing set ID",
            "source_name": "VCF",
            "source_version": "VCFv4.2"
        },
        "AF": {
            "title": "AF",
            "vcf_name": "AF",
            "type": "number",
            "description": "Sample allele fraction",
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "column_priority": 5
        },
        "RSTR": {
            "title": "RSTR",
            "vcf_name": "RSTR",
            "type": "string",
            "description": "Reference forward allele read count",
            "source_name": "novoCaller",
            "source_version": "2"
        },
        "schema_version": {
            "default": "1"
        },
        "sample": {
            "title": "Sample",
            "type": "string",
            "linkTo": "Sample"
        },
        "variant": {
            "title": "Variant",
            "type": "string",
            "linkTo": "Variant"
        }
    },
    "columns": {
        "DP": {
            "title": "DP"
        },
        "AF": {
            "title": "AF"
        }
    },
    "facets": {
        "DP": {
            "title": "DP"
        },
        "AF": {
            "title": "AF"
        }
    }
}