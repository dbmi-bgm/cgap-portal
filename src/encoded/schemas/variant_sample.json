{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": [
        "institution",
        "project"
    ],
    "identifyingProperties": [
        "uuid",
        "aliases"
    ],
    "additionalProperties": false,
    "mixinProperties": [
        {
            "$ref": "mixins.json#/schema_version"
        },
        {
            "$ref": "mixins.json#/uuid"
        },
        {
            "$ref": "mixins.json#/aliases"
        },
        {
            "$ref": "mixins.json#/submitted"
        },
        {
            "$ref": "mixins.json#/modified"
        },
        {
            "$ref": "mixins.json#/status"
        },
        {
            "$ref": "mixins.json#/attribution"
        },
        {
            "$ref": "mixins.json#/notes"
        },
        {
            "$ref": "mixins.json#/static_embeds"
        }
    ],
    "title": "Sample Variant",
    "description": "Schema for variant info for sample",
    "id": "/profiles/variant_sample.json",
    "properties": {
        "QUAL": {
            "title": "QUAL",
            "vcf_name": "QUAL",
            "type": "number",
            "description": "Quality score",
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Variant Quality",
            "schema_description": "Quality score",
            "value_example": "302.6",
            "scope": "sample_variant"
        },
        "FILTER": {
            "title": "FILTER",
            "vcf_name": "FILTER",
            "type": "string",
            "description": "VQSR filter",
            "enum": [
                "PASS",
                "VQSRTrancheSNP99.00to99.90",
                "VQSRTrancheSNP99.90to100.00",
                "LowQual"
            ],
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Variant Quality",
            "schema_description": "VQSR filter",
            "value_example": "PASS;VQSRTrancheSNP99.00to99.90;VQSRTrancheSNP99.90to100.00",
            "scope": "sample_variant"
        },
        "CALL_INFO": {
            "title": "CALL_INFO",
            "vcf_name": "CALL_INFO",
            "type": "string",
            "description": "Calling info",
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Variant Quality",
            "schema_description": "Calling info",
            "value_example": "0/1:33,14:47:99:310,0,927",
            "scope": "sample_variant"
        },
        "GT": {
            "title": "GT",
            "vcf_name": "GT",
            "type": "string",
            "description": "Genotype",
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Genotype",
            "schema_description": "Genotype",
            "value_example": "0/1,0/2,1/2",
            "scope": "sample_variant"
        },
        "AD": {
            "title": "AD",
            "vcf_name": "AD",
            "type": "string",
            "description": "Allelic depth",
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Variant Quality",
            "schema_description": "Allelic depth",
            "value_example": "33,14",
            "scope": "sample_variant"
        },
        "AD_ref": {
            "title": "AD_ref",
            "vcf_name": "AD_ref",
            "type": "integer",
            "description": "Reference allelic depth",
            "calculated_property": true,
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Variant Quality",
            "schema_description": "Reference allelic depth",
            "value_example": "33",
            "scope": "sample_variant"
        },
        "AD_alt": {
            "title": "AD_alt",
            "vcf_name": "AD_alt",
            "type": "integer",
            "description": "Alternate allelic depth",
            "calculated_property": true,
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Variant Quality",
            "schema_description": "Alternate allelic depth",
            "value_example": "14",
            "scope": "sample_variant"
        },
        "DP": {
            "title": "DP",
            "vcf_name": "DP",
            "type": "integer",
            "description": "Approximate read depth",
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Variant Quality",
            "schema_description": "Approximate read depth",
            "value_example": "47",
            "scope": "sample_variant"
        },
        "GQ": {
            "title": "GQ",
            "vcf_name": "GQ",
            "type": "integer",
            "description": "Genotype quality score",
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Variant Quality",
            "schema_description": "Genotype quality score",
            "value_example": "99",
            "scope": "sample_variant"
        },
        "PL": {
            "title": "PL",
            "vcf_name": "PL",
            "type": "string",
            "description": "Phred-scaled likelihoods for genotype (Ref/Ref, Ref/Alt, Alt/Alt)",
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Variant Quality",
            "schema_description": "Phred-scaled likelihoods for genotype (Ref/Ref, Ref/Alt, Alt/Alt)",
            "value_example": "310,200,100",
            "scope": "sample_variant"
        },
        "PGT": {
            "title": "PGT",
            "vcf_name": "PGT",
            "type": "string",
            "description": "Physical phasing haplotype information",
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Genotype",
            "schema_description": "Physical phasing haplotype information",
            "value_example": "0|1",
            "scope": "sample_variant"
        },
        "PID": {
            "title": "PID",
            "vcf_name": "PID",
            "type": "string",
            "description": "Physical phasing ID information",
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Genotype",
            "schema_description": "Physical phasing ID information",
            "value_example": "985446_G_T",
            "scope": "sample_variant"
        },
        "PS": {
            "title": "PS",
            "vcf_name": "PS",
            "type": "integer",
            "description": "Phasing set ID",
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Genotype",
            "schema_description": "Phasing set ID",
            "value_example": "985446",
            "scope": "sample_variant"
        },
        "AF": {
            "title": "AF",
            "vcf_name": "AF",
            "type": "number",
            "description": "Sample allele fraction",
            "calculated_property": true,
            "source_name": "VCF",
            "source_version": "VCFv4.2",
            "annotation_category": "Variant Quality",
            "schema_description": "Sample allele fraction",
            "value_example": "0.2979",
            "scope": "sample_variant"
        },
        "samplegeno": {
            "title": "Sample Genotype",
            "type": "array",
            "items": {
                "title": "Sample Genotype",
                "type": "object",
                "properties": {
                    "NUMGT": {
                        "title": "NUMGT",
                        "vcf_name": "NUMGT",
                        "type": "string",
                        "description": "Sample genotype (number format)",
                        "source_name": "SAMPLEGENO",
                        "source_version": "v0.4",
                        "sub_embedding_group": "{\"key\": \"samplegeno\", \"title\": \"Sample Genotype\"}",
                        "schema_description": "Sample genotype (number format)",
                        "value_example": "0/1",
                        "scope": "sample_variant"
                    },
                    "GT": {
                        "title": "GT",
                        "vcf_name": "GT",
                        "type": "string",
                        "description": "Sample genotype (nucletide format)",
                        "source_name": "SAMPLEGENO",
                        "source_version": "v0.4",
                        "sub_embedding_group": "{\"key\": \"samplegeno\", \"title\": \"Sample Genotype\"}",
                        "schema_description": "Sample genotype (nucletide format)",
                        "value_example": "G/T",
                        "scope": "sample_variant"
                    },
                    "AD": {
                        "title": "AD",
                        "vcf_name": "AD",
                        "type": "string",
                        "description": "Allelic depth",
                        "source_name": "SAMPLEGENO",
                        "source_version": "v0.4",
                        "sub_embedding_group": "{\"key\": \"samplegeno\", \"title\": \"Sample Genotype\"}",
                        "annotation_category": "Variant Quality",
                        "schema_description": "Allelic depth",
                        "value_example": "21/10",
                        "scope": "sample_variant"
                    },
                    "SAMPLEID": {
                        "title": "SAMPLEID",
                        "vcf_name": "SAMPLEID",
                        "type": "string",
                        "description": "Sample ID",
                        "source_name": "SAMPLEGENO",
                        "source_version": "v0.4",
                        "sub_embedding_group": "{\"key\": \"samplegeno\", \"title\": \"Sample Genotype\"}",
                        "schema_description": "Sample ID",
                        "scope": "sample_variant"
                    }
                }
            }
        },
        "schema_version": {
            "default": "1"
        },
        "sample": {
            "title": "Sample",
            "type": "string",
            "linkTo": "Sample"
        },
        "variant": {
            "title": "Variant",
            "type": "string",
            "linkTo": "Variant"
        }
    },
    "columns": {
        "variant.CHROM": {
            "title": "CHROM"
        },
        "variant.POS": {
            "title": "POS"
        },
        "variant.ID": {
            "title": "ID"
        },
        "variant.hgvs_hgvsg": {
            "title": "hgvs_hgvsg"
        },
        "variant.transcript.vep_consequence.display_title": {
            "title": "vep_consequence"
        },
        "variant.transcript.vep_symbol": {
            "title": "vep_symbol"
        },
        "variant.transcript.vep_variant_class": {
            "title": "vep_variant_class"
        },
        "variant.conservation_phastcons100": {
            "title": "conservation_phastcons100",
            "aggregation_type": "stats",
            "number_step": "any",
            "grouping": "conservation"
        },
        "variant.cadd_phred": {
            "title": "cadd_phred",
            "aggregation_type": "stats",
            "number_step": "any",
            "grouping": "pathogenicity"
        },
        "variant.max_pop_af_af_popmax": {
            "title": "max_pop_af_af_popmax",
            "aggregation_type": "stats",
            "number_step": "any",
            "grouping": "population frequency"
        }
    },
    "facets": {
        "variant.transcript.vep_consequence.display_title": {
            "title": "vep_consequence"
        },
        "variant.transcript.vep_symbol": {
            "title": "vep_symbol"
        },
        "variant.transcript.vep_hgvsc": {
            "title": "vep_hgvsc"
        },
        "variant.transcript.vep_variant_class": {
            "title": "vep_variant_class"
        },
        "variant.conservation_phastcons100": {
            "title": "conservation_phastcons100",
            "aggregation_type": "stats",
            "number_step": "any",
            "grouping": "conservation"
        },
        "variant.cadd_phred": {
            "title": "cadd_phred",
            "aggregation_type": "stats",
            "number_step": "any",
            "grouping": "pathogenicity"
        },
        "variant.max_pop_af_af_popmax": {
            "title": "max_pop_af_af_popmax",
            "aggregation_type": "stats",
            "number_step": "any",
            "grouping": "population frequency"
        }
    }
}