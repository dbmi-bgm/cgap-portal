{
    "title": "Somatic Analysis",
    "description": "Somatic analysis of samples",
    "id": "/profiles/somatic_analysis.json",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": [
        "project",
        "institution"
    ],
    "identifyingProperties": [
        "uuid",
        "aliases",
        "accession"
    ],
    "additionalProperties": false,
    "mixinProperties": [
        {
            "$ref": "abstract_analysis#/properties"
        }
    ],
    "properties": {
        "schema_version": {
            "default": "1"
        },
        "accession": {
            "accessionType": "SS"
        },
        "samples": {
            "title": "Samples",
            "description": "Samples used in the analysis",
            "type": "array",
            "items": {
                "title": "Sample",
                "type": "string",
                "linkTo": "Sample"
            }
        }
    }
}
