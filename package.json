    "higlass-general-vcf": "0.1.1",
    "higlass-gnomad": "0.2.1",
    "higlass-orthologs": "0.2.3",
    "higlass-pileup": "1.4.1",
    "higlass-register": "^0.3.0",
    "higlass-sequence": "0.3.5",
    "higlass-sv": "1.0.3",
    "higlass-text": "0.1.3",
    "higlass-transcripts": "0.3.3",
    "markdown-to-jsx": "^6.11.4",
    "memoize-one": "^5.1.1",
    "pagedjs": "^0.2.0",
    "pixi.js": "^5.2.4",
    "prop-types": "^15.8.1",
    "query-string": "^4.3.4",
    "react": "^17.0.2",
    "react-bootstrap": "1.6.4",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.4",
    "react-jsx-parser": "^1.25.1",
    "react-redux": "^7.2.6",
    "react-tooltip": "4.2.10",
